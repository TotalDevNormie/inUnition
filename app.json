{
  "expo": {
    "name": "inUnition",
    "slug": "inunition",
    "scheme": "inunition",
    "version": "1.0.0",
    "userInterfaceStyle": "dark",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon.png"
    },
    "androidStatusBar": {
      "translucent": true
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true,
      "turboModules": true
    },
    "orientation": "portrait",
    "assetBundlePatterns": ["**/*"],
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoSquare.png",
        "monochromeImage": "./assets/LogoSquareMono.png",
        "backgroundColor": "#313749"
      },
      "package": "com.total_normie.inunition"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "32478bc0-845f-4e39-a556-a26a34a22f91"
      }
    },
    "ios": {
      "bundleIdentifier": "com.total-normie.inunition"
    }
  }
}
